{
  "name": "miaoxing/category",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Category Plugin",
  "keywords": [
    "miaoxing", "category"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.2.2",
    "miaoxing/link-to": "0.1.2"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Category\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Category\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
